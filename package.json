{
	"displayName": "nyaakube | Nyaarium",
	"name": "nyaakube",
	"version": "3.0.0",
	"license": "MIT",
	"scripts": {
		"====[ Build ]====": "",
		"dev": "scripts/migrationUp.sh && scripts/loadEnv.sh npx next dev",
		"start": "NODE_ENV=production scripts/migrationUp.sh && scripts/loadEnv.sh npx next start",
		"build": "npm run clean && npx next build",
		"migrate:generate": "scripts/migrationGenerate.sh",
		"lint": "npx next lint",
		"test": "jest",
		"coverage": "jest --coverage",
		"patch": "npm --no-git-tag-version version patch",
		"minor": "npm --no-git-tag-version version minor",
		"major": "npm --no-git-tag-version version major",
		"====[ Misc ]====": "",
		"reinstall": "npm run purge && npm i",
		"update": "npx npm-check-updates -u && npm run reinstall",
		"clean": "rm -rf .next/",
		"purge": "rm -rf .next/ node_modules/ package-lock.json"
	},
	"dependencies": {
		"@types/node": "^22.10.2",
		"pg": "^8.13.1",
		"ts-node": "^10.9.2",
		"typeorm": "^0.3.20"
	},
	"devDependencies": {
		"@emotion/react": "^11.14.0",
		"@emotion/styled": "^11.14.0",
		"@levminer/speakeasy": "^1.4.2",
		"@mui/icons-material": "^6.2.1",
		"@mui/material": "^6.2.1",
		"@testing-library/jest-dom": "^6.6.3",
		"@types/express": "^5.0.0",
		"@types/express-session": "^1.18.1",
		"@types/jest": "^29.5.14",
		"@types/lodash": "^4.17.13",
		"@types/react": "^19.0.2",
		"@types/saslprep": "^1.0.3",
		"@types/uuid": "^10.0.0",
		"argon2": "^0.41.1",
		"connect-pg-simple": "^10.0.0",
		"dotenv": "^16.4.7",
		"eslint": "^9.17.0",
		"eslint-config-next": "^15.1.2",
		"eslint-plugin-security": "^3.0.1",
		"express-session": "^1.18.1",
		"graphql": "^16.10.0",
		"jest": "^29.7.0",
		"jest-environment-jsdom": "^29.7.0",
		"json-to-graphql-query": "^2.3.0",
		"json5": "^2.2.3",
		"lodash": "^4.17.21",
		"moment": "^2.30.1",
		"moment-timezone": "^0.5.46",
		"next": "^15.1.2",
		"react": "^19.0.0",
		"saslprep": "^1.0.3",
		"uuid": "^11.0.3"
	}
}
